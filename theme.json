  "styles": {
    "blocks": {
      "core/pullquote": {
        "elements": {
          "cite": {

            "typography": {
              "fontFamily": "var(--wp--preset--font-family--open-sans)",
              "fontWeight": "500"
            }

          }
        },
        "color": {
          "background": "var(--wp--preset--color--blue)",
          "text": "var(--wp--preset--color--lavender)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--lato)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "600"
        }
      },
      "core/quote": {

        "border": {
          "color": "var(--wp--preset--color--pink)",
          "width": "0 0 0 12px"
        },
        "color": {
          "background": "var(--wp--preset--color--lavender)",
          "text": "var(--wp--preset--color--blue)"

        },
        "spacing": {

          "padding": {
            "bottom": "var(--wp--preset--spacing--30)",
            "right": "var(--wp--preset--spacing--30)",
            "top": "var(--wp--preset--spacing--30)",
            "left": "var(--wp--preset--spacing--30)"
          }
        }
      },
      "core/separator": {
        "border": {
          "width": "1px"
        }
      },
      "core/site-title": {
        "typography": {
          "fontWeight": "400",
          "textTransform": "uppercase",
          "fontStyle": "normal",
          "fontSize": "var(--wp--preset--font-size--x-large)"
        }
      }
    },
	"color": {
		"background": "var(--wp--preset--color--white)",
		"text": "var(--wp--preset--color--gray)"
	},
    "elements": {
      "button": {
        "color": {
					"background": "var(--wp--preset--color--orange)",
					"text": "var(--wp--preset--color--white)"
				},
        "typography": {
					"fontWeight": "500"
        },
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--white)",
            "gradient": "var(--wp--preset--gradient--luminous-vivid-amber-to-luminous-vivid-orange)"
					}
				},
				":focus": {
					"color": {
            "gradient": "var(--wp--preset--gradient--luminous-vivid-amber-to-luminous-vivid-orange)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":active": {
					"color": {
            "gradient": "var(--wp--preset--gradient--luminous-vivid-amber-to-luminous-vivid-orange)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--white)"
					}
				}
			},
			"heading": {
				"color": {
          "text": "var(--wp--preset--color--blue)"
				}
			},
      "link": {
				"color": {
					"text": "var(--wp--preset--color--orange)"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--orange)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
    },
    "spacing": {
      "padding": {
        "top": "0",
        "right": "0",
        "bottom": "0",
        "left": "0"
      }
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		}
	}
}
